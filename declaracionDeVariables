package clase1;

public class Ej3 {

     public static void main(String[] args) {
         // Lo que esta a la derecha de barras dobles es un comentario
         
         
         //ALGUNOS TIPOS PRIMITIVOS
         int a; //Esto es un numero entero
         a=10;
         System.out.println(a); //Imprime a por consola
         
         float b; //Esto es un decimal
         b=5.3F;  
         System.out.println(b); //Imprime b por consola
         
         double c; //Esto es un decimal de doble presicion
         c=1200030.231; 
         System.out.println(b);
         
         char d;  //Esto es un caracter
         d='Z';
         System.out.println(d);
         
         //TIPO OBJETO
         
         String e;  //Esto es una cadena de caracteres
         e="Hola!";
         System.out.println(e);
         

     }
}
